<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/enquiry" method="POST">
        <service class="Freshbox\Contact\Api\EnquiryRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
	 <route url="/V1/enquiry/:enquiryId" method="GET">
        <service class="Freshbox\Contact\Api\EnquiryRepositoryInterface" method="getById"/>
        <resources>
            <!-- <resource ref="anonymous" /> -->
            <resource ref="Freshbox_Contact::enquiry" />
        </resources>
    </route>

    <route url="/V1/enquiry/search" method="GET">
        <service class="Freshbox\Contact\Api\EnquiryRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
	<route url="/V1/enquiry/:enquiryId" method="DELETE">
        <service class="Freshbox\Contact\Api\EnquiryRepositoryInterface" method="deleteById"/>
        <resources>
            <resource ref="Freshbox_Contact::enquiry_delete" />
        </resources>
    </route>
</routes>