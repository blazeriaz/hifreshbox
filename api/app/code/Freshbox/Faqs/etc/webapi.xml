<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/faqs/:faqId" method="GET">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="getById"/>
        <resources>
            <!-- <resource ref="anonymous" /> -->
            <resource ref="Freshbox_Faqs::faq" />
        </resources>
    </route>

    <route url="/V1/faqs/search" method="GET">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
	<route url="/V1/faqs" method="POST">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="save"/>
        <resources>
            <resource ref="Freshbox_Faqs::faq_save" />
        </resources>
    </route>
    
     <route url="/V1/faqs/change-order" method="POST">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="changeorder"/>
        <resources>
            <resource ref="Freshbox_Faqs::faq_save" />
        </resources>
    </route>
	
	<route url="/V1/faqs/:faqId" method="PUT">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="save"/>
        <resources>
            <resource ref="Freshbox_Faqs::faq_save"/>
        </resources>
    </route>

    <route url="/V1/faqs/:faqId" method="DELETE">
        <service class="Freshbox\Faqs\Api\FaqRepositoryInterface" method="deleteById"/>
        <resources>
            <resource ref="Freshbox_Faqs::faq_delete" />
        </resources>
    </route>
</routes>