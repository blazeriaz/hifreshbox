<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/newsletter/search" method="GET">
        <service class="Freshbox\Newsletter\Api\SubscriberRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
	
	<route url="/V1/newsletter/add" method="POST">
        <service class="Freshbox\Newsletter\Api\SubscriberRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
	
	<route url="/V1/customer/addnewaddress/:customerId" method="POST">
        <service class="Freshbox\Newsletter\Api\SubscriberRepositoryInterface" method="newaddress"/>
        <resources>
             <resource ref="self"/>
        </resources>
    </route>
	
</routes>